    "    \"def clean_text(text):\\n\",\n",
    "    \"  clean_txt = re.sub(r'http\\\\S+', '', text) # Remove URLs\\n\",\n",
    "    \"  clean_txt = re.sub(r'@[A-Za-z0-9]+', '', clean_txt) # Remove mentions\\n\",\n",
    "    \"  clean_txt = re.sub(r'#', '', clean_txt) # Remove hashtags\\n\",\n",
    "    \"  clean_txt = re.sub(r'[^A-Za-z0-9\\\\s]+', '', clean_txt) # Remove special characters and punctuation\\n\",\n",
    "    \"  clean_txt = re.sub(r'RT|cc|CC|rt', '', clean_txt)\\n\",\n",
    "    \"  clean_txt=re.sub(r'\\\\s+', ' ', clean_txt) # Remove extra spaces\\n\",\n",
    "    \"  return clean_txt\"\n",
